{
  "name": "classmentors",
  "version": "0.1.1",
  "description": "Class Mentors UI",
  "repository": "github:singpath/classmentors",
  "main": "src/classmentors/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.35",
    "babel-eslint": "^5.0.0-beta8",
    "chai": "^3.5.0",
    "compression": "^1.6.1",
    "eslint": "^1.10.3",
    "finalhandler": "^0.4.1",
    "http2": "^3.3.2",
    "jscs": "^2.9.0",
    "jspm": "^0.16.31",
    "mocha": "^2.4.5",
    "morgan": "^1.7.0",
    "npm-run-all": "^1.5.3",
    "serve-static": "^1.10.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "config": {
    "port": 8081,
    "build_dir": "dist/"
  },
  "scripts": {
    "build": "npm-run-all clean build:dir build:assets build:archive",
    "build:archive": "cd ${npm_package_config_build_dir}; zip -r ${npm_package_name}.zip ${npm_package_name}",
    "build:assets": "npm-run-all --parallel build:assets:dist build:assets:ace build:assets:bundles",
    "build:assets:ace": "cp src/jspm_packages/github/ajaxorg/ace-builds@*/worker-*.js ${npm_package_config_build_dir}/${npm_package_name}/",
    "build:assets:dist": "cp -r assets/* ${npm_package_config_build_dir}/${npm_package_name}/",
    "build:assets:bundles": "jspm depcache app.js && jspm bundle-sfx app.js ${npm_package_config_build_dir}/${npm_package_name}/app.js --minify",
    "build:dir": "mkdir -p ${npm_package_config_build_dir}/${npm_package_name}",
    "clean": "test -d $npm_package_config_build_dir && rm -r $npm_package_config_build_dir || echo 'Nothing to clean.'",
    "keygen": "openssl req -x509 -nodes -newkey rsa:4096 -new -days 1024 -subj \"/C=SG/ST=Denial/L=Singapore/O=Dis/CN=localhost\" -keyout bin/localhost.key -out bin/localhost.cert",
    "lint": "eslint src/ && jscs src/",
    "serve": "./bin/server.js src/",
    "serve-build": "./bin/server.js dist/classmentors",
    "start": "./bin/server.js",
    "test": "mocha --compilers js:babel/register tests/index"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "jspm": {
    "main": "index.js",
    "directories": {
      "baseURL": "src",
      "lib": "src/classmentors"
    },
    "dependencies": {
      "ace": "github:ajaxorg/ace-builds@^1.2.3",
      "angular": "github:angular/bower-angular@^1.5.0",
      "angular-animate": "github:angular/bower-angular-animate@^1.5.0",
      "angular-loading-bar": "github:chieffancypants/angular-loading-bar@^0.8.0",
      "angular-material": "github:angular/bower-material@^1.0.6",
      "angular-messages": "github:angular/bower-angular-messages@^1.5.0",
      "angular-route": "github:angular/bower-angular-route@^1.5.0",
      "angularfire": "github:firebase/angularfire@^1.1.4",
      "cryptojs": "github:sytelus/cryptojs@^3.1.2",
      "firebase": "github:firebase/firebase-bower@^2.4.1",
      "singpath-core": "github:singpath/singpath-core@^0.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.10",
      "core-js": "npm:core-js@^1.1.4",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "text": "github:systemjs/plugin-text@^0.0.7"
    },
    "overrides": {
      "github:sytelus/cryptojs@3.1.2": {
        "main": "core.js",
        "format": "global",
        "directories": {
          "lib": "components"
        },
        "shim": {
          "core.js": {
            "exports": "CryptoJS"
          },
          "sha224.js": {
            "deps": [
              "cryptojs/sha256.js"
            ]
          },
          "sha512.js": {
            "deps": [
              "cryptojs/x64-core.js"
            ]
          },
          "sha384.js": {
            "deps": [
              "cryptojs/x64-core.js",
              "cryptojs/sha512.js"
            ]
          },
          "sha3.js": {
            "deps": [
              "cryptojs/x64-core.js"
            ]
          },
          "hmac-md5.js": {
            "deps": [
              "cryptojs/md5.js"
            ]
          },
          "hmac-sha1.js": {
            "deps": [
              "cryptojs/sha1.js"
            ]
          },
          "hmac-sha256.js": {
            "deps": [
              "cryptojs/sha256.js"
            ]
          },
          "hmac-sha224.js": {
            "deps": [
              "cryptojs/sha256.js",
              "cryptojs/sha224.js"
            ]
          },
          "hmac-sha512.js": {
            "deps": [
              "cryptojs/x64-core.js",
              "cryptojs/sha512.js"
            ]
          },
          "hmac-sha384.js": {
            "deps": [
              "cryptojs/x64-core.js",
              "cryptojs/sha512.js",
              "cryptojs/sha384.js"
            ]
          },
          "hmac-sha3.js": {
            "deps": [
              "cryptojs/x64-core.js",
              "cryptojs/sha3.js"
            ]
          },
          "hmac-ripemd160.js": {
            "deps": [
              "cryptojs/ripemd160.js"
            ]
          },
          "pbkdf2.js": {
            "deps": [
              "cryptojs/sha1.js",
              "cryptojs/hmac.js"
            ]
          },
          "aes.js": {
            "deps": [
              "cryptojs/enc-base64.js",
              "cryptojs/md5.js",
              "cryptojs/evpkdf.js",
              "cryptojs/cipher-core.js"
            ]
          },
          "tripledes.js": {
            "deps": [
              "cryptojs/enc-base64.js",
              "cryptojs/md5.js",
              "cryptojs/evpkdf.js",
              "cryptojs/cipher-core.js"
            ]
          },
          "rc4.js": {
            "deps": [
              "cryptojs/enc-base64.js",
              "cryptojs/md5.js",
              "cryptojs/evpkdf.js",
              "cryptojs/cipher-core.js"
            ]
          },
          "rabbit.js": {
            "deps": [
              "cryptojs/enc-base64.js",
              "cryptojs/md5.js",
              "cryptojs/evpkdf.js",
              "cryptojs/cipher-core.js"
            ]
          },
          "rabbit-legacy.js": {
            "deps": [
              "cryptojs/enc-base64.js",
              "cryptojs/md5.js",
              "cryptojs/evpkdf.js",
              "cryptojs/cipher-core.js"
            ]
          },
          "*": {
            "deps": [
              "cryptojs/core.js"
            ]
          }
        }
      },
      "github:ajaxorg/ace-builds@1.2.3": {
        "main": "ace.js",
        "format": "global",
        "directories": {
          "lib": "src-noconflict"
        },
        "shim": {
          "ace.js": {
            "exports": "ace"
          },
          "*": {
            "deps": [
              "ace/ace.js"
            ]
          }
        }
      },
      "github:firebase/angularfire@1.1.4": {
        "directories": {
          "lib": "dist"
        },
        "main": "angularfire.js",
        "shim": {
          "*": {
            "deps": [
              "firebase",
              "angular"
            ]
          }
        },
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.5.0",
          "firebase": "^2.4.0"
        }
      }
    }
  }
}
